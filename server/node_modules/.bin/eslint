#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules/eslint/bin/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules/eslint/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/node_modules:/Users/yam/Desktop/学习/node_modules:/Users/yam/Desktop/node_modules:/Users/yam/node_modules:/Users/node_modules:/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/eslint/bin/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/eslint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules/eslint/bin/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules/eslint/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/eslint@8.23.0/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/.pnpm/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/node_modules:/Users/yam/Desktop/学习/node_modules:/Users/yam/Desktop/node_modules:/Users/yam/node_modules:/Users/node_modules:/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/eslint/bin/node_modules:/Users/yam/Desktop/学习/宠物网站-全栈/full-stack-pet-website/server/node_modules/eslint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
